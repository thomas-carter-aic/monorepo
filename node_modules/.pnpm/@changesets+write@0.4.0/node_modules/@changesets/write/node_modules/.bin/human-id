#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/human-id@4.1.1/node_modules/human-id/dist/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/human-id@4.1.1/node_modules/human-id/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/human-id@4.1.1/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/human-id@4.1.1/node_modules/human-id/dist/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/human-id@4.1.1/node_modules/human-id/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/human-id@4.1.1/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../human-id@4.1.1/node_modules/human-id/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../human-id@4.1.1/node_modules/human-id/dist/cli.js" "$@"
fi
