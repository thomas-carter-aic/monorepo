#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_57b29e83b776fc5a0de83e455f0517f9/node_modules/ts-jest/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_57b29e83b776fc5a0de83e455f0517f9/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_57b29e83b776fc5a0de83e455f0517f9/node_modules/ts-jest/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@30.0.4_@jest+types@30.0.1_babel-jest@_57b29e83b776fc5a0de83e455f0517f9/node_modules:/home/oss/Repos/AIC/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
